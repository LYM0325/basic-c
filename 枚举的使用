enum menu {
    EXIT,
    ADD,
    SUB,
    MUL,
    DIV
};
int main() {
	int input = 0;
	do {
		printf("请输入：");
		scanf("%d",&input);
		switch(input) {
			case ADD:
				break;
			case SUB:
				break;
			case MUL:
				break;
			case DIV:
				break;
			case EXIT:
				break;
		}
	} while(input);
}
