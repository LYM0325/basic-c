#include<stdio.h>
//这是一个失败的函数，因为函数内的x,y虽然进行了交换，但是对于外面的a,b并没有进行交换 
void swap1(int x ,int y)
{
	int temp = 0;
	temp = x;
	x = y;
	y = temp;
}
void swap2(int * pa,int * pb)
{
	int temp = 0;
	temp = *pa;
	*pa = *pb;
	*pb = temp;
 } 
int main()
{
	int a = 10;
	int b = 20;
	swap1(a,b);
	printf("%d %d\n",a,b);
	swap2(&a,&b);
	printf("%d %d\n",a,b); 
	return 0;
}
