#include<stdio.h>
int main()
{ 
	int n = 9;
	float* f = (float*)&n;
	printf("%d\n",n);//9
	printf("%f\n",*f);//0.00
	//整型的存储    00000000 00000000 00000000 00001001
	//浮点型的存储  0 00000000 00000000000000000001001
	//              s e        m  
	//0.00000000000000000001001*2^(-126),无限接近0的数 
	*f = 9.0;
	//浮点型的存储 1001.0->1.001*2^3
	//0 10000010 00100000000000000000000
	//s e        m
	printf("%d\n",n);
	//以整数的形式输出，那就是一个很大的正整数
	printf("%f\n",*f);//9.0 
	return 0;
} 
